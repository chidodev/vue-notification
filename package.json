{
  "name": "vue-notifications__gh-pages",
  "version": "0.0.1",
  "homepage": "https://github.com/se-panfilov/vue-notifications",
  "description": "demo site",
  "main": "index.html",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/se-panfilov/vue-notifications.git"
  },
  "keywords": [
    "non-blocking",
    "notification",
    "notifications",
    "toaster",
    "Toastr",
    "toastr",
    "mini-toastr",
    "mini-toaster",
    "miniToastr",
    "miniToaster",
    "toastrjs",
    "toastr.js",
    "mini-toastr.js",
    "messages",
    "errors"
  ],
  "authors": [
    "S.Panfilov <se-panfilov@ya.ru> (https://se-panfilov.github.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/se-panfilov/vue-notifications/issues"
  },
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "mini-toastr": "^0.3.10",
    "vue": "^2.0.1",
    "vue-notifications": "^0.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.25.0",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.7.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "peerDependencies": {
    "stylus": "^0.54.5"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
